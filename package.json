{
  "name": "blogs-scan",
  "version": "1.0.0",
  "description": "Web scraper for cafef.vn with ChatGPT integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "extract-links": "ts-node src/cli/extract-links.ts",
    "crawl-links": "ts-node src/cli/crawl-links.ts",
    "extract-build": "node dist/cli/extract-links.js",
    "crawl-build": "node dist/cli/crawl-links.js",
    "migrate": "ts-node src/scripts/run-migrations.ts"
  },
  "keywords": [
    "scraper",
    "chatgpt",
    "postgres"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
